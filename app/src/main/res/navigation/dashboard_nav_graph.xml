<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/dashboard_nav_graph"
    app:startDestination="@id/homeScreenFragment">

    <fragment
        android:id="@+id/homeScreenFragment"
        android:name="com.example.sunnzaraa.ui.bottomBar.HomeScreenFragment"
        android:label="fragment_home_screen"
        tools:layout="@layout/fragment_home_screen" >
        <action
            android:id="@+id/action_homeScreenFragment_to_profileScreenFragment"
            app:destination="@id/profileScreenFragment" />
        <action
            android:id="@+id/action_homeScreenFragment_to_libraryScreenFragment"
            app:destination="@id/libraryScreenFragment" />
        <action
            android:id="@+id/action_homeScreenFragment_to_premiumScreenFragment"
            app:destination="@id/premiumScreenFragment" />
        <action
            android:id="@+id/action_homeScreenFragment_to_searchScreenFragment"
            app:destination="@id/searchScreenFragment" />
        <action
            android:id="@+id/action_homeScreenFragment_to_musicLanguagesScreenFragment"
            app:destination="@id/musicLanguagesScreenFragment" />
        <action
            android:id="@+id/action_homeScreenFragment_to_settingsScreenFragment"
            app:destination="@id/settingsScreenFragment" />
        <action
            android:id="@+id/action_homeScreenFragment_to_termsPrivacyScreenFragment"
            app:destination="@id/termsPrivacyScreenFragment" />
        <action
            android:id="@+id/action_homeScreenFragment_to_helpFaqScreenFragment"
            app:destination="@id/helpFaqScreenFragment" />
        <action
            android:id="@+id/action_homeScreenFragment_to_editProfileScreenFragment"
            app:destination="@id/editProfileScreenFragment" />
        <action
            android:id="@+id/action_homeScreenFragment_to_favouritesScreenFragment"
            app:destination="@id/favouritesScreenFragment" />
        <action
            android:id="@+id/action_homeScreenFragment_to_albumScreenFragment"
            app:destination="@id/albumScreenFragment" />
    </fragment>
    <fragment
        android:id="@+id/profileScreenFragment"
        android:name="com.example.sunnzaraa.ui.profile.ProfileScreenFragment"
        android:label="fragment_profile_screen"
        tools:layout="@layout/fragment_profile_screen" />
    <fragment
        android:id="@+id/libraryScreenFragment"
        android:name="com.example.sunnzaraa.ui.bottomBar.LibraryScreenFragment"
        android:label="fragment_library_screen"
        tools:layout="@layout/fragment_library_screen" />
    <fragment
        android:id="@+id/searchScreenFragment"
        android:name="com.example.sunnzaraa.ui.bottomBar.SearchScreenFragment"
        android:label="fragment_search_screen"
        tools:layout="@layout/fragment_search_screen" />
    <fragment
        android:id="@+id/premiumScreenFragment"
        android:name="com.example.sunnzaraa.ui.bottomBar.PremiumScreenFragment"
        android:label="fragment_premium_screen"
        tools:layout="@layout/fragment_premium_screen" />
    <fragment
        android:id="@+id/musicLanguagesScreenFragment"
        android:name="com.example.sunnzaraa.ui.drawer.MusicLanguagesScreenFragment"
        android:label="fragment_music_languages_screen"
        tools:layout="@layout/fragment_music_languages_screen" />
    <fragment
        android:id="@+id/settingsScreenFragment"
        android:name="com.example.sunnzaraa.ui.drawer.SettingsScreenFragment"
        android:label="fragment_settings_screen"
        tools:layout="@layout/fragment_settings_screen" />
    <fragment
        android:id="@+id/termsPrivacyScreenFragment"
        android:name="com.example.sunnzaraa.ui.drawer.TermsPrivacyScreenFragment"
        android:label="fragment_terms_privacy_screen"
        tools:layout="@layout/fragment_terms_privacy_screen" />
    <fragment
        android:id="@+id/helpFaqScreenFragment"
        android:name="com.example.sunnzaraa.ui.drawer.HelpFaqScreenFragment"
        android:label="fragment_help_faq_screen"
        tools:layout="@layout/fragment_help_faq_screen" />
    <fragment
        android:id="@+id/editProfileScreenFragment"
        android:name="com.example.sunnzaraa.ui.profile.EditProfileScreenFragment"
        android:label="fragment_edit_profile_screen"
        tools:layout="@layout/fragment_edit_profile_screen" />
    <fragment
        android:id="@+id/favouritesScreenFragment"
        android:name="com.example.sunnzaraa.ui.FavouritesScreenFragment"
        android:label="fragment_favourites_screen"
        tools:layout="@layout/fragment_favourites_screen" />
    <fragment
        android:id="@+id/albumScreenFragment"
        android:name="com.example.sunnzaraa.ui.AlbumScreenFragment"
        android:label="fragment_album_screen"
        tools:layout="@layout/fragment_album_screen" />
</navigation>